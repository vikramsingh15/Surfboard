<%layout("/partials/boilerplate")-%>
<div class="container">
	
	<form action="/register" method="POST" enctype="multipart/form-data">
	  <div class="form-group row">
	    <label for="a" class="col-sm-2 col-form-label">username</label>
	    <div class="col-sm-10">
	      <input type="text" name="username" value="<%=username%>" class="form-control" id="a" placeholder="username" required>
	    </div>
	  </div>
	  <div class="form-group row">
	    <label for="b" class="col-sm-2 col-form-label">email</label>
	    <div class="col-sm-10">
	    <input type="text" class="form-control" id="b" placeholder="email" name="email" value="<%=email%>" required>
	    </div>
	  </div>
	  <div class="form-group row">
	    <label for="g" class="col-sm-2 col-form-label">avatar</label>
	    <div class="col-sm-10">
	      <input type="file" id="g" name="avatar" />
	    </div>
	  </div>
	  <div class="form-group row">
	    <label for="c" class="col-sm-2 col-form-label">password</label>
	    <div class="col-sm-10">
	      <input type="password" class="form-control" id="c" placeholder="password" name="password">
	    </div>
	  </div>
	  
	  <div class="form-group row">
	    <div class="col-sm-10">
	      <button type="submit" class="btn btn-primary">submit</button>
	    </div>
	  </div>
	</form>

</div>

